{
  "name": "nivasesa",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo dev --concurrency=15",
    "dev:rent": "turbo dev --filter=@niv/rent-app",
    "dev:lead": "turbo dev --filter=@niv/lead-gen",
    "build": "turbo build",
    "build:rent": "turbo build --filter=@niv/rent-app",
    "build:lead": "turbo build --filter=@niv/lead-gen",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css}\"",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/qrcode": "^1.5.6",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^23.0.1",
    "otpauth": "^9.2.2",
    "prettier": "^3.7.4",
    "qrcode": "^1.5.3",
    "turbo": "^2.5.4",
    "typescript": "^5",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  }
}
